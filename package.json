{
  "name": "zenci-microservice-router",
  "version": "0.2.0",
  "description": "URL router for micro services",
  "main": "router-admin.js",
  "scripts": {
    "test": "node_modules/.bin/mocha --timeout 15000",
    "start-admin": "DEBUG=http:log,cluster:* node  --max-old-space-size=24 router-admin.js >> $LOGFILE 2>&1 &",
    "start-proxy": "DEBUG=http:log,cluster:* node  --max-old-space-size=24 router-proxy.js >> $LOGFILE 2>&1 &",
    "stop-admin": "kill -2 `cat $PIDFILE` 2>&1 &",
    "stop-proxy": "kill -2 `cat $PIDFILE.proxy` 2>&1 &",
    "jscs": "jscs --fix router-admin.js router-proxy.js",
    "devel-start-admin": "DEBUG=* node  --max-old-space-size=24 router-admin.js",
    "devel-start-proxy": "DEBUG=* node  --max-old-space-size=24 router-proxy.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itpatrol/zenci-microservice-router.git"
  },
  "author": "Gormartsen",
  "license": "SEE LICENSE IN <LICENSE.txt>",
  "bugs": {
    "url": "https://github.com/itpatrol/zenci-microservice-router/issues"
  },
  "homepage": "https://github.com/itpatrol/zenci-microservice-router#readme",
  "dependencies": {
    "debug": "^2.2.0",
    "dotenv": "^2.0.0",
    "zenci-manager": "github:itpatrol/zenci-manager",
    "zenci-microservice": "github:itpatrol/zenci-microservice"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.0.0",
    "zenci-microservice-client": "github:itpatrol/zenci-microservice-client"
  }
}
