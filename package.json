{
  "name": "@microservice-framework/microservice-router",
  "version": "1.3.1",
  "description": "Microservice framework router",
  "main": "router-admin.js",
  "scripts": {
    "test": "node_modules/.bin/mocha --timeout 15000",
    "start-admin": "DEBUG=http:log,cluster:*,router:log node  --max-old-space-size=520 router-admin.js",
    "start-proxy": "DEBUG=http:log,cluster:*,proxy:log node  --max-old-space-size=520 router-proxy.js",
    "stop-admin": "ADMIN=true node kill.js",
    "stop-proxy": "PROXY=true node kill.js",
    "status": "node status.js",
    "cs": "eslint --fix *.js includes/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microservice-framework/microservice-router.git"
  },
  "author": "Gormartsen",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/microservice-framework/microservice-router/issues"
  },
  "homepage": "https://github.com/microservice-framework/microservice-router#readme",
  "dependencies": {
    "@microservice-framework/microservice": "github:microservice-framework/microservice#2.x",
    "@microservice-framework/microservice-client": "^1.3.0",
    "@microservice-framework/microservice-cluster": "github:microservice-framework/microservice-cluster#2.x",
    "@microservice-framework/microservice-router-register": "github:microservice-framework/microservice-router-register#2.x",
    "debug": "^2.2.0",
    "dot": "^1.1.1",
    "dotenv": "^2.0.0",
    "fs-extra": "^3.0.1"
  },
  "devDependencies": {
    "@microservice-framework/microservice-client": "^1.3.5",
    "chai": "^3.5.0",
    "mocha": "^3.0.0"
  }
}
